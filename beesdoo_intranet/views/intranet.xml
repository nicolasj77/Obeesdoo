<openerp>
	<data>
		<record id="intranet_product_tree_view" model="ir.ui.view">
			<field name="name">Intranet product tree view</field>
			<field name="model">product.template</field>
			<field name="arch" type="xml">
				<tree string="Products">
					<field name="name" />
					<field name="total_with_vat" string="Sale Price TTC" />
					<field name="main_seller_id" />
					<field name="eco_label" />
					<field name="local_label" />
					<field name="fair_label" />
					<field name="origin_label" />
				</tree>
			</field>
		</record>

		<record id="intranet_product_form_view" model="ir.ui.view">
			<field name="name">Intranet product form view</field>
			<field name="model">product.template</field>
			<field name="inherit_id" ref="product.product_template_only_form_view"/>
			<field name="arch" type="xml">
				<field name="" 
			</field>
		</record>

		<record model="ir.actions.act_window" id="action_intranet_list_products">
			<field name="name">Products</field>
			<field name="res_model">product.template</field>
			<field name="view_mode">tree,form</field>
		</record>

		<menuitem id="menu_intranet" name="Intranet" sequence="0" />
		<menuitem id="menu_intranet_products" parent="menu_intranet"
			name="Products" action="action_intranet_list_products" />

		<!--<record id="intranet_contact_form" model="ir.ui.view"> <field name="name">My 
			Data</field> <field name="model">beesdoo.intranet</field> <field name="arch" 
			type="xml"> <tree string="Contact"> <field name="partner_id" /> </tree> </field> 
			</record> <record model="ir.actions.act_window" id="action_data"> <field 
			name="name">My Data</field> <field name="res_model">beesdoo.intranet</field> 
			<field name="view_mode">form</field> </record> -->

	</data>
</openerp>