<openerp>
	<data>
		<record id="intranet_product_list" model="ir.ui.view">
			<field name="name">Products</field>
			<field name="model">product.template</field>
			<field name="arch" type="xml">
				<tree string="Products">
					<field name="name" />
					<field name="eco_label" />
					<field name="local_label" />
					<field name="fair_label" />
					<field name="origin_label" />
				</tree>
			</field>
		</record>
		
		<record model="ir.actions.act_window" id="action_list_products">
			<field name="name">Products</field>
			<field name="res_model">product.template</field>
			<field name="view_mode">tree</field>

		</record>


		<menuitem id="menu_intranet" name="Intranet" />
		<menuitem id="menu_intranet_products" parent="menu_intranet"
			name="Products" sequence="10" action="action_list_products" />
	</data>
</openerp>